<%-include ("./partials/header")%>

<table class="table table-hover table-light">
    <thead class="thead-dark">
    <tr>
        <th scope="col">Sl. No.</th>
        <th scope="col">Name</th>
        <th scope="col">Roll Number</th>
    </tr>
    </thead>
    <tbody>
        <%let num = 1%>
        <%users.forEach(user => {%>
            <tr>
                <th scope="row"><%=num%></th>
                <td><%=user.name%></td>
                <td><%=user.rollno%></td>
            </tr>
            <%num = num + 1%>
        <%})%>
    </tbody>
</table>

<%-include ("./partials/footer")%>